# https://dev.to/milandhar/an-intro-to-ruby-gems-ap8
source 'https://rubygems.org'
gem 'sinatra'

# "groups", set to default by default
# [:development, :test]
gem 'sorbet', '0.4.4386', :group => :development
gem 'sorbet-runtime', '0.4.4386'
# now bundle install, all gems in the gemfile will be installed

# Gemfile.lock is a snapshot of the gem versions
